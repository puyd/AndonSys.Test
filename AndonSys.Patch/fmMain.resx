    <value>ALTER procedure [dbo].[P_NEWLABEL] 
(
	@plan int
)
AS
BEGIN
	declare @prod int =(
		select B01_PROD_CODE from V_B01_PLAN where B01_PLAN_ID=@plan
	)
	
	declare @pnm varchar(50)=(
		select A07_PROD_NAME from A07_PROD where A07_PROD_CODE=@prod
	)
	
	declare @n int=(
		select A40_LABEL_NO from A40_LABEL where A40_PROD_CD=@prod
	)
	
	declare @rate varchar(10)=(
		select A40_LABEL_RATE from A40_LABEL where A40_PROD_CD=@prod
	)
	
	declare @fit varchar(8)=
	(
		select A40_FIT_NO from A40_LABEL where A40_PROD_CD=@prod
	)
	
	declare @no varchar(5)=right(@n+100000,5);
	
	declare @sup varchar(5)=(
		select top 1 B02_SUP_CODE from V_B02_PLAN_BOM where B02_PLAN_ID=@plan order by B02_PROC_ID,B02_FIT_ID
	)
	
	declare @y varchar(2)=right(datepart(year,getdate()),2);
	
	declare @m varchar(2)=datepart(MONTH,getdate());
	
	if (@m='10') set  @m='A'
	if (@m='11') set  @m='B'
	if (@m='12') set  @m='C'
	
	update A40_LABEL set A40_LABEL_NO=A40_LABEL_NO+1 where A40_PROD_CD=@prod
	
	declare @cd varchar(50)= @y+@m+@no+@sup+@fit+'-'+@pnm+'-'+@rate
	--declare @lb varchar(50)= @y+@m+@no+@pnm+'-'+@rate
	declare @lb varchar(50)= @y+@m+@no+'ZJ'+@pnm+'-'+@rate
	
	select @cd CODE,@lb LABEL
END
</value>
